{
    "builds": [
        {
            "src": "api/publish.js",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/((?!api/|_next/|static/|public/|favicon.ico).*)",
            "destination": "/index.html"
        }
    ]
}