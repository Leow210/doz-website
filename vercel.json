{
    "functions": {
        "api/publish.js": {
            "includeFiles": "package.json"
        }
    },
    "rewrites": [
        {
            "source": "/api/publish",
            "destination": "/api/publish"
        },
        {
            "source": "/((?!api/|_next/|static/|public/|favicon.ico).*)",
            "destination": "/index.html"
        }
    ]
}